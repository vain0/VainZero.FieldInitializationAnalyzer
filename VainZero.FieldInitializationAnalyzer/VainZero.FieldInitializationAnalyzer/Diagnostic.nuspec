<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>VainZero.FieldInitializationAnalyzer</id>
    <version>1.0.0</version>
    <title>VainZero.FieldInitializationAnalyzer</title>
    <authors>vain0</authors>
    <owners>vain0</owners>
    <licenseUrl>https://github.com/vain0/VainZero.FieldInitializationAnalyzer/blob/master/LICENSE.md</licenseUrl>
    <projectUrl>https://github.com/vain0/VainZero.FieldInitializationAnalyzer</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Analyzer to ensure constructors initialize fields.</description>
    <releaseNotes></releaseNotes>
    <copyright>(C) 2017 vain0</copyright>
    <tags>analyzers</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\portable50 and language specific analyzers in either analyzers\portable50\cs or analyzers\portable50\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>